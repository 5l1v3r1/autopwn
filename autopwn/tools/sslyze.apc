name: sslyze
binary_location: /usr/bin/sslyze
url: https://github.com/iSECPartners/sslyze
description: Fast and full-featured SSL scanner.
autopwn_version: 0.17.0
assessment_groups: ['ssl']

rules:
    equals:
        'protocol': 'https'
    target-parameter-exists: ['target_name', 'target', 'port_number']

arguments: "--regular {target}:{port_number} > {output_dir}/{date}_{name}_sslyze_{target_name}"
stdout: False
